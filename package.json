{
  "name": "cadre",
  "description": "",
  "author": "CACI International Inc",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "private-repo": "git+ssh://somewhere:port/folder/on/server"
  },
  "main": "app.js",
  "readmeFilename": "",
  "engines": {
    "node": ">=8.0.0"
  },
  "engineStrict": true,
  "dependencies": {
    "check-dependencies": "1.1.0",
    "colors": "1.1.2",
    "roosevelt":"~0.11.4",
    "roosevelt-less":"~0.4.0",
    "roosevelt-uglify":"~0.2.0",
    "teddy":"~0.4.0"
  },
  "devDependencies": {
    "husky": "~0.14.3",
    "lint-staged": "~6.0.0",
    "nodemon": "~1.14.7",
    "reload": "2.2.2",
    "standard": "~10.0.3",
    "stylelint": "~8.4.0",
    "stylelint-config-standard": "~18.0.0"
  },
  "rooseveltConfig": {
    "port": 43712,
    "generateFolderStructure": true,
    "localhostOnly": true,
    "suppressLogs": {
      "httpLogs": false,
      "rooseveltLogs": false,
      "rooseveltWarnings": false
    },
    "noMinify": false,
    "htmlValidator": {
      "enable": true,
      "port": 8888,
      "exceptions": {
        "requestHeader": "Partial",
        "modelValue": "_disableValidator"
      },
      "separateProcess": false,
      "suppressWarnings": false
    },
    "multipart": {
      "multiples": true
    },
    "toobusy": {
      "maxLagPerRequest": 70,
      "lagCheckInterval": 500
    },
    "shutdownTimeout": 30000,
    "bodyParserUrlencodedParams": {
      "extended": true
    },
    "bodyParserJsonParams": {},
    "https": {
      "enable": false,
      "httpsOnly": false,
      "httpsPort": 43733,
      "pfx": false,
      "keyPath": "null",
      "passphrase": "null",
      "ca": "null",
      "cafile": true,
      "requestCert": false,
      "rejectUnauthorized": false
    },
    "modelsPath": "mvc/models",
    "viewsPath": "mvc/views",
    "viewEngine": ["html: teddy"],
    "controllersPath": "mvc/controllers",
    "error404": "404.js",
    "error5xx": "5xx.js",
    "error503": "503.js",
    "staticsRoot": "statics",
    "htmlMinify": {
      "override": true,
      "exception_url": false,
      "htmlMinifier": {
        "html5": true
      }
    },
    "css": {
      "sourceDir": "css",
      "compiler": {"nodeModule":"roosevelt-less","params":{"cleanCSS":{"advanced":true,"aggressiveMerging":true},"sourceMap":null}},
      "whitelist": null,
      "output": ".build/css",
      "versionFile": null
    },
    "js": {
      "sourceDir": "js",
      "compiler": {"nodeModule":"roosevelt-uglify","showWarnings":false,"params":{}},
      "whitelist": null,
      "blacklist": null,
      "output": ".build/js",
      "bundler": {
        "bundles": [],
        "output": ".bundled",
        "expose": true
      }
    },
    "publicFolder": "public",
    "favicon": "images/favicon.ico",
    "staticsSymlinksToPublic": [
      "images",
      "fonts",
      "css: .build/css",
      "js: .build/js"
    ],
    "versionedPublic": false,
    "alwaysHostPublic": false
  },
  "nodemonConfig": {
    "ignore": [
      "**/.build/",
      "statics/js/.bundled/",
      "public/",
      "jsdoc/"
    ],
    "ext": "less html js json"
  },
  "scripts": {
    "start": "npm run production",
    "production": "nodemon app.js --production-mode",
    "prod": "npm run production",
    "p": "npm run production",
    "development": "nodemon app.js --development-mode",
    "dev": "npm run development",
    "d": "npm run development",
    "kill-validator": "node ./node_modules/roosevelt/lib/scripts/killValidator.js",
    "kv": "npm run kill-validator",
    "clean": "node ./node_modules/roosevelt/lib/scripts/appCleanup.js",
    "audit": "node ./node_modules/roosevelt/lib/scripts/configAuditor.js",
    "standard": "standard",
    "stylelint": "./node_modules/.bin/stylelint \"statics/css/**/*.less\" --syntax less",
    "lint": "npm run standard && npm run stylelint",
    "test": "npm run lint",
    "precommit": "lint-staged",
    "pc": "npm run precommit"
  },
  "lint-staged": {
    "*.less": "./node_modules/.bin/stylelint --syntax less",
    "*.js": "standard"
  }
}
